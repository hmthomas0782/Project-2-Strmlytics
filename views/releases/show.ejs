<%- include("../partials/header.ejs") %>

<h1><%= release.title %></h1>
<p>Artist: <%= release.artist %></p>
<p>Release Date: <%= release.releaseDate.toLocaleDateString() %></p>

<a href="/releases/<%= release._id %>/edit">Edit</a>
<form action="/releases/<%= release._id %>?_method=DELETE" method="POST" style="display: inline;">
  <button type="submit">Delete</button>
</form>
<a href="/releases">Back to All Releases</a>

<%- include("../partials/footer.ejs") %>














